package org.cisco.cmad.BloggingApp.api;

import java.net.URI;

public class Link {
	
			URI uri;
			String reference;
			public URI getUri() {
				return uri;
			}
			public void setUri(URI uri2) {
				this.uri = uri2;
			}
			public String getRel() {
				return rel;
			}
			public void setRel(String rel) {
				this.rel = rel;
			}
			
					

}
